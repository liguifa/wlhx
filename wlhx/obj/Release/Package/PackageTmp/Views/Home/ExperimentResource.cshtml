    <link href="~/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!-- start plugins -->
    <script type="text/javascript" src="~/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/js/bootstrap.js"></script>
    <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
    <!----font-Awesome----->
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <!----font-Awesome----->
    <style>
        #general {
            width: 86%;
            text-indent: 2em;
            margin: auto;
        }
    </style>
</head>
<body>
    <div class="header_bg1">
        <div class="container">
            <div class="row header">
                <div class="logo navbar-left">
                    <h1><a href="/Home/Index">无机材料实验教学中心 </a></h1>
                    <p>Experimental Teaching Center of Inorganic Materials</p>
                </div>
                <div class="h_search navbar-right">
                    <form action="http://cn.bing.com/search" target="_blank">
                        <input type="text" class="text" name="q" value="搜索内容" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '搜索内容';}">
                        <input type="submit" value="搜索">
                    </form>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    @Html.Partial("~/Views/Home/Head.cshtml")
    <script>
        $("#li7").addClass("active");
        window.onload = function ()
        {
            var a = navigator.appVersion;
            var patt1 = new RegExp(/MSIE \d\d?/);
            var b = patt1.exec(a) + "";
            b = parseInt(b.split("MSIE ")[1])
            if (b < 9)
            {
                location.href = "/Home/BrowserError";
            }
        }
    </script>
    <div class="clearfix"></div>

    <div class="main_bg">
        <!-- start main -->
        <!-- On rows -->
        <div class="content_right" id="general">
            @{
                foreach (wlhx.Models.Dynamic d in ViewBag.resList)
                {
                    <div class="panel panel-default" style="margin-top:15px;">
                        <div class="panel-heading">@d.dynamic_title</div>
                        <div class="panel-body">
                            <script>
                                var html = "@d.dynamic_body";
                                document.write(html.replace(/&lt;/g, "<").replace(/&gt;/g, ">").replace(/&quot;/g, "\"").replace(/amp;/g, ""));
                            </script>
                        </div>
                    </div>
                }
            }


        </div>
    </div><!-- end main -->
    @Html.Partial("~/Views/Home/foot.cshtml")
</body>
</html>