@{
    wlhx.Models.Dynamic d = ViewBag.msg;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="keywords" content="大连工业大学,无机材料实验教学中心,无机材料,实验教学中心,无机材料研究所,辽宁省重点实验室,硅酸盐工程实验室" />
    <meta name="description" content="大连工业大学无机材料实验室，原为硅酸盐工程实验室，始建于1958年，是我校建校初始就创建的实验室之一。经过50余年的发展，已经逐步形成了以玻璃、陶瓷材料为教学实验，以无机新材料为研究实验的特色人才培养体系，注重实践与创新能力的培养和建设，并取得了辉煌的成绩。" />
    <title>@d.dynamic_title</title>

    <!-- Bootstrap -->
    <link href="~/css/bootstrap.css" rel='stylesheet' type='text/css' />
    <link href="~/css/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--[if lt IE 9]>
         <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
         <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="~/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!-- start plugins -->
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <script type="text/javascript" src="/js/bootstrap.js"></script>
    <script type="text/javascript" src="/js/bootstrap.min.js"></script>
    <!----font-Awesome----->
    <link rel="stylesheet" href="/css/font-awesome.min.css">
    <!----font-Awesome----->
    <style type="text/css">
        #general {
            width: 86%;
            text-indent: 2em;
            margin: auto;
            margin-top: 30px;
        }

            #general > .title {
                text-align: center;
            }
    </style>
    <!--导入pdfobject.js插件-->
    <script src="~/js/pdfobject.min.js"></script>
    <script type="text/javascript">

        window.onload = function ()
        {
            //sample.pdf是要显示的文件。
            var myPDF = new PDFObject({ url: "../../@d.dynamic_body" }).embed();
        };

    </script>

</head>
<body>
    <div class="header_bg1">
        <div class="container">
            <div class="row header">
                <div class="logo navbar-left">
                    <h1><a href="/Home/Index">无机材料实验教学中心 </a></h1>
                    <p>Experimental Teaching Center of Inorganic Materials</p>
                </div>
                <div class="h_search navbar-right">
                    <form action="http://cn.bing.com/search" target="_blank">
                        <input type="text" class="text" name="q" value="搜索内容" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '搜索内容';}">
                        <input type="submit" value="搜索">
                    </form>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    @Html.Partial("~/Views/Home/Head.cshtml")

    <div class="clearfix"></div>
    <div class="main_bg">
        <!-- start main -->
        <div class="content_right" id="general">
            <h3>
                你的浏览器暂时不支持在线查看，你可以直接下载<a href="/Home/ResposeFile/@d.dynamic_id">下载该pdf文件</a>
            </h3>
        </div>
    </div><!-- end main -->
    @Html.Partial("~/Views/Home/foot.cshtml")


</body>

</html>